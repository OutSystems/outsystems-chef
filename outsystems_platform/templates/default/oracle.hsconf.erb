<EnvironmentConfiguration>
  <PlatformDatabaseConfiguration ProviderKey="Oracle">

    <Host encrypted="false"><%= @outsystems_platform.database.server %></Host>
    <ServiceName encrypted="false"><%= @outsystems_platform.database.service_name %></ServiceName>
    <Port encrypted="false"><%= @outsystems_platform.database.port %></Port>

    <AdminUser encrypted="false"><%= @outsystems_platform.database.admin_user %></AdminUser>
    <RuntimeUser encrypted="false"><%= @outsystems_platform.database.runtime_user %></RuntimeUser>
    <LogUser encrypted="false"><%= @outsystems_platform.database.log_user %></LogUser>

    <AdminPassword encrypted="false"><%= @outsystems_platform.database.admin_password %></AdminPassword>
    <RuntimePassword encrypted="false"><%= @outsystems_platform.database.runtime_password %></RuntimePassword>
    <LogPassword encrypted="false"><%= @outsystems_platform.database.log_password %></LogPassword>
  
    <AdminTablespace encrypted="false"><%= @outsystems_platform.database.admin_tablespace %></AdminTablespace>
    <IndexTablespace encrypted="false"><%= @outsystems_platform.database.index_tablespace %></IndexTablespace>
    <RuntimeTablespace encrypted="false"><%= @outsystems_platform.database.runtime_tablespace %></RuntimeTablespace>
    <LogTablespace encrypted="false"><%= @outsystems_platform.database.log_tablespace %></LogTablespace>

    <NLS_Territory encrypted="false">AMERICA</NLS_Territory>
    <NLS_Language encrypted="false"></NLS_Language>

    <ServicesAdvancedSettings encrypted="false"></ServicesAdvancedSettings>
    <IntrospectionMethod encrypted="false">auto</IntrospectionMethod>
    <CI_AI encrypted="false">true</CI_AI>
    <DateFunction encrypted="false">CURRENT_TIMESTAMP</DateFunction>
    <AdvancedConfigurationMode encrypted="false">false</AdvancedConfigurationMode>
    <RuntimeAdvancedSettings encrypted="false"></RuntimeAdvancedSettings>
    <DDLLockTimeout encrypted="false">90</DDLLockTimeout>
  </PlatformDatabaseConfiguration>
  <SessionDatabaseConfiguration ProviderKey="Oracle">

    <Host encrypted="false"><%= @outsystems_platform.session_database.server %></Host>
    <ServiceName encrypted="false"><%= @outsystems_platform.session_database.service_name %></ServiceName>
    <Port encrypted="false"><%= @outsystems_platform.session_database.port %></Port>


    <SessionUser encrypted="false"><%= @outsystems_platform.session_database.session_user %></SessionUser>
    <SessionPassword encrypted="false"><%= @outsystems_platform.session_database.session_password %></SessionPassword>

    <SessionTablespace encrypted="false"><%= @outsystems_platform.session_database.session_tablespace %></SessionTablespace>

    <NLS_Territory encrypted="false">AMERICA</NLS_Territory>
    <NLS_Language encrypted="false"></NLS_Language>

    <IntrospectionMethod encrypted="false"></IntrospectionMethod>
    <SessionAdvancedSettings encrypted="false"></SessionAdvancedSettings>
    <CI_AI encrypted="false">false</CI_AI>
    <DateFunction encrypted="false"></DateFunction>
    <AdvancedConfigurationMode encrypted="false">false</AdvancedConfigurationMode>
    <DDLLockTimeout encrypted="false">90</DDLLockTimeout>
  </SessionDatabaseConfiguration>
  <ServiceConfiguration>
    <CompilerServerHostname><%= @outsystems_platform.compiler_hostname %></CompilerServerHostname>

    <CompilerServerPort>12000</CompilerServerPort>
    <DeploymentServerPort>12001</DeploymentServerPort>
    <JDBCPoolInactivityTimeout>300</JDBCPoolInactivityTimeout>
    <JDBCPoolMaxPoolSize>100</JDBCPoolMaxPoolSize>
    <JDBCPoolMinPoolSize>10</JDBCPoolMinPoolSize>
    <LogQueuePath>queue/OutSystemsLog</LogQueuePath>
    <LogServerPort>12003</LogServerPort>
    <RMIRegistryPort>2033</RMIRegistryPort>
    <SchedulerServerPort>12002</SchedulerServerPort>
    <SupportAsynchronousLog>true</SupportAsynchronousLog>
  </ServiceConfiguration>
  <ServerConfiguration>
    <ApplicationServerPort>80</ApplicationServerPort>
    <ApplicationServerSecurePort>443</ApplicationServerSecurePort>
    <MaxConcurrentTimers>3</MaxConcurrentTimers>
    <WeeksToKeep>4</WeeksToKeep>
  </ServerConfiguration>
  <NetworkConfiguration>
    <ApplicationServerVersion>(Unknown version)</ApplicationServerVersion>
    <FrontendIpAddress></FrontendIpAddress>
    <InternalNetworkIPs></InternalNetworkIPs>
    <OutgoingIPAddress></OutgoingIPAddress>
    <RequiresSoapHeadersSlowRetrieval>false</RequiresSoapHeadersSlowRetrieval>
    <ServerKind>JBoss</ServerKind>
  </NetworkConfiguration>
  <OtherConfigurations>
    <DBIntrospectionTimeout>90</DBIntrospectionTimeout>
    <DBTimeout>30</DBTimeout>
    <DBUpdateTimeout>600</DBUpdateTimeout>
    <InstallationDir>/opt/outsystems/platform</InstallationDir>
    <InstanceName>DEFAULT</InstanceName>
    <PlatformVersion>9.1.0.8</PlatformVersion>
    <QueuesAvailable>false</QueuesAvailable>
  </OtherConfigurations>
  <ExtensionPointConfigurations>
    <OutSystems.Wildfly.DeployDir></OutSystems.Wildfly.DeployDir>
  </ExtensionPointConfigurations>
</EnvironmentConfiguration>